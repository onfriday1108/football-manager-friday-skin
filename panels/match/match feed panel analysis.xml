<panel>

	<boolean id="cancel_mouse_hide" value="true"/>
	<integer id="max_feed_items" value="-1"/>
	<boolean id="always_enable_feed" value="true"/>

	<record id="match_feed_item_panel_properties">
		<string id="file" value="match/match feed item panel analysis"/>
		<boolean id="dfou" value="true"/>
	</record>

	<list id="specific_notification_types">
		<integer value="14"/>
		<integer value="15"/>
		<integer value="16"/>
		<integer value="17"/>
	</list>

	<layout class="arrange_vertical_attachment" alignment="bottom,extend" offset="0"/>
	<layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" apply_to_children="true" />

	<container class="scrolling_box">
		<!--standard feed items in here-->
		<container id="feed">
			<attachment_group class="vertical_arrange_autosize" vertical_alignment="top"/>
		</container>
	</container>

	<!--link-->
	<container height="30">
		<layout class="arrange_horizontal_attachment" alignment="right" offset="0"/>
		<layout class="stick_to_sides_attachment" alignment="vertical" inset="0" apply_to_children="true" />

		<widget class="link_button" id="flnk" auto_size="horizontal"/>
	</container>
</panel>