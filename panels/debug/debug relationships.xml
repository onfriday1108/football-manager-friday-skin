<panel>

	<string id="title" value="Relationships Debug" />
	<layout class="stick_to_sides_attachment" alignment="all" inset="0" />

	<!-- table -->
	<container class="main_box" id="titl">
		<string id="title" value="Relationships Debug" />
		<layout class="stick_to_sides_attachment" alignment="all" inset="0" />

		<container class="scrolling_box">
			<layout class="stick_to_sides_attachment" alignment="all" inset="0" />
			<widget class="table" id="ditb" layout="-1, -1, -1" mode="fill_rows, stripe_rows,printable" auto_size_rows="true" fixed_size_rows="false" row_spacing="0" auto_size="vertical">

				<layout class="fit_children_attachment" alignment="vertical" embed_in_frame="true" />

				<list id="headings">
					<record alignment="left,centre_y">
						<string id="text" value="Person" />
					</record>
					<record alignment="left,centre_y">
						<string id="text" value="Linked" />
					</record>
					<record alignment="left,centre_y">
						<string id="text" value="Level" />
					</record>
				</list>

				<record id="default_properties">
					<flags id="alignment" value="left,centre_y" />
				</record>

				<list id="column_table_properties">
					<record indx="0" sort_disabled="true" />
					<record indx="1" sort_disabled="true" />
					<record indx="2" sort_disabled="true" />
				</list>

			</widget>
		</container>
	</container>

</panel>