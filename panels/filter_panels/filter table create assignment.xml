<panel layout="1, 1, 1, 1, -1, 90, -1, 35, -1, 20" auto_size_rows="false" fixed_size_rows="true" row_height="30">

	<flags id="mode" value="fill_rows"/>

	<!-- column display properties -->
	<list id="column_table_properties">

		<!-- enabled column -->
		<record indx="0" left="0" top="4">
			<flags id="id" value="enab"/>
		</record>

		<!-- operator column -->
		<record indx="1" left="0">
			<flags id="id" value="oprt" />
		</record>

		<!-- move rule up column -->
		<record indx="2" left="0">
			<flags id="id" value="murl" />
			<flags id="column_alignment" value="centre" />
		</record>

		<!-- move rule down column -->
		<record indx="3" left="0">
			<flags id="id" value="mdrl" />
			<flags id="column_alignment" value="centre" />
		</record>

		<!-- rule type column -->
		<record indx="4" left="0">
			<flags id="id" value="rlty" />
		</record>

		<!-- comparison type column -->
		<record indx="5" left="0">
			<flags id="id" value="cmty" />
		</record>

		<!-- value column -->
		<record indx="6" left="0">
			<flags id="id" value="val1" />
		</record>

		<!-- and column -->
		<record indx="7" left="0">
			<flags id="id" value="andc" />
		</record>

		<!-- second value column -->
		<record indx="8" left="0">
			<flags id="id" value="val2" />
		</record>

		<!-- remove rule column -->
		<record indx="9" left="0">
			<flags id="id" value="rmrl" />
		</record>
	</list>
</panel>
<!-- table -->
