<panel layout="50,150, 80, 60, 35, 60" auto_size_rows="false" fixed_size_rows="true">

	<flags id="mode" value="fill_rows"/>

	<!-- column display properties -->
	<list id="column_table_properties">

		<!-- operator column -->
		<record indx="0" left="5">
			<flags id="id" value="oprt" />
		</record>

		<!-- rule type column -->
		<record indx="1" left="5">
			<flags id="id" value="rlty" />
		</record>

		<!-- comparison type column -->
		<record indx="2" left="5">
			<flags id="id" value="cmty" />
		</record>

		<!-- value column -->
		<record indx="3" left="5">
			<flags id="id" value="val1" />
		</record>

		<!-- and column -->
		<record indx="4" left="5">
			<flags id="id" value="andc" />
		</record>

		<!-- second value column -->
		<record indx="5" left="5">
			<flags id="id" value="val2" />
		</record>


	</list>
</panel>
