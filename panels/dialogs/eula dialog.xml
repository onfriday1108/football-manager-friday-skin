<panel can_close_with_escape="false">
	<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />
	<layout class="centre_in_parent_attachment" alignment="all" />

	<container class="multi_box" no_controls="true" id="main" contents_kind="container" default_item="selc">
		<container id="selc">
			<layout class="arrange_vertical_attachment" alignment="top,extend" />
			<layout class="stick_to_sides_attachment" alignment="horizontal" apply_to_children="true" />

			<container>

				<layout class="stick_to_sides_attachment" alignment="horizontal" apply_to_children="true" inset="8" />
				<layout class="stick_to_sides_attachment" alignment="vertical" apply_to_children="true" inset="8" />

				<container>
					<layout class="arrange_vertical_attachment" alignment="bottom,extend" gap="8" />
					<layout class="stick_to_sides_attachment" alignment="horizontal" apply_to_children="true" />

					<container>
						<attachment_group class="vertical_arrange" vertical_alignment="top,extend" vertical_gap="8" vertical_offset="0" horizontal_inset="0" />

						<container class="main_box">
							<attachment_group class="vertical_arrange_autosize" />

							<translation id="title" translation_id="454702" type="use" value="Privacy Policies" />

							<!--<widget class="button" id="EUbt" auto_size="vertical">
								<translation id="text" translation_id="381798" type="use" value="End User License Agreement" />
							</widget>-->
							<widget class="button" id="PPbt" auto_size="vertical">
								<translation id="text" translation_id="469722" type="use" value="Privacy Policy (for customers)[COMMENT: EULA text]" />
							</widget>
							<widget class="button" id="TSbt" auto_size="vertical">
								<translation id="text" translation_id="469723" type="use" value="Privacy Policy (for football professionals)[COMMENT: EULA text]" />
							</widget>
							<widget class="button" id="BSbt" auto_size="vertical">
								<translation id="text" translation_id="495441" type="use" value="Privacy Policy (Bidstack)[COMMENT: AREA: EULA; SUB-AREA: Legal text; FR-BN: n/a; TONE: Formal; CHARLIMIT: None; REF-ID: 469723; NOTE: Privacy Policy specific to Bidstack, leave that company name as-is, use the referenced ID for the other bit]" />
							</widget>

							<widget class="spacer" height="10" />


							<container>
								<attachment_group class="horizontal_arrange_vertical_autosize" horizontal_alignment="left,extend" />

								<widget class="spacer" width="20" hight="20" />

								<widget class="check_box" id="PPck" auto_size="vertical" select_event="PPsl" multiline="true">
									<translation id="text" translation_id="469724" type="use" value="I acknowledge that I have read and understood the practices set out in the Privacy Policy that applies to me.[COMMENT: EULA text, cannot be feminised]" />
								</widget>
							</container>

						</container>

						<container>
							<attachment_group class="vertical_arrange" vertical_alignment="bottom,extend" vertical_gap="8" vertical_offset="0" horizontal_inset="0" />

							<container class="main_box">
								<translation id="title" translation_id="469725" type="use" value="Your Choice Regarding Match Day Advertising Hoardings[COMMENT: EULA text, cannot be feminised]" />
								<attachment_group class="vertical_arrange" vertical_alignment="bottom,extend" vertical_gap="10" />

								<container class="scrolling_box">
									<container>
										<attachment_group class="vertical_arrange_autosize" vertical_gap="10" />
										
										<widget class="text" id="bsPT" auto_size="vertical" multiline="true">
											<translation id="text" translation_id="495544" type="use" value="Want to see advertising on match hoardings that makes more sense to you? If you choose 'Yes' below, we will share user data (including a personal unique identifier) and information about your game usage with third parties to display targeted advertising, including Bidstack Limited whose privacy policy is available to read [%string#1]. Those third party advertisers may combine this with other information they have collected about you from third parties to serve you more relevant advertisements. If you choose 'No', you will still see advertising but this will not be based on your inferred interests or preferences.[COMMENT: AREA: Game EULA; SUB-AREA: Bidstack Privacy Policy; FR-BN: n/a; TONE: Formal; CHARLIMIT: None; REF-ID: 469726; NOTE: updated Bidstack privacy policy, referenced ID has most of this, the string variable will be populated by the string arriving with this in your batch that says 'here' and references Bidstack in the comment]" />
										</widget>

										<widget class="text" auto_size="vertical" multiline="true">
											<translation id="text" translation_id="469727" type="use" value="To change your mind and later opt-out at any time, please refer to the Preferences screen page.[COMMENT: EULA text]" />
										</widget>
									</container>
								</container>

								<container>
									<attachment_group class="horizontal_arrange_vertical_autosize" horizontal_alignment="right" horizontal_gap="-8" horizontal_offset="0" vertical_inset="0" />

									<widget class="radio_toggle_switch_left" id="opin" alignment="centre" click_event="OPIN" auto_size="all">
										<translation id="text" translation_id="242044" type="use" value="Yes" />
									</widget>
									<widget class="radio_toggle_switch_right" id="opou" alignment="centre" click_event="OPOU" auto_size="all">
										<translation id="text" translation_id="231460" type="use" value="No" />
									</widget>

								</container>

							</container>

							<container class="main_box" id="ageg">
								<translation id="title" translation_id="469469" type="use" value="Age Gate[COMMENT: EULA dialog; bug 185228, cannot be feminised]" />
								<attachment_group class="vertical_arrange_autosize" vertical_gap="8" />

								<widget class="text" auto_size="vertical" multiline="true">
									<translation id="text" translation_id="469728" type="use" value="To ensure that advertising on match hoardings is age appropriate, we will share your age with third party advertisers.[COMMENT: EULA Age Gate text, cannot be feminised]" />
								</widget>

								<widget class="text" auto_size="vertical" multiline="true">
									<translation id="text" translation_id="469729" type="use" value="We do not collect and/or share any other user data or behaviour information if you are under the age of 13.[COMMENT: EULA Age Gate text, cannot be feminised]" />
								</widget>

								<container>
									<attachment_group class="horizontal_arrange_vertical_autosize" horizontal_alignment="right" />

									<widget class="popup_button" auto_size="all" sele="sdbm" id="monp">
										<translation id="text" translation_id="469467" type="use" value="Select Month[COMMENT: EULA dialog; bug 185228]" />
										<list id="items">
											<record id="1">
												<translation id="name" translation_id="249618" type="use" value="January" />
											</record>
											<record id="2">
												<translation id="name" translation_id="249619" type="use" value="February" />
											</record>
											<record id="3">
												<translation id="name" translation_id="249620" type="use" value="March" />
											</record>
											<record id="4">
												<translation id="name" translation_id="249621" type="use" value="April" />
											</record>
											<record id="5">
												<translation id="name" translation_id="249622" type="use" value="May" />
											</record>
											<record id="6">
												<translation id="name" translation_id="249623" type="use" value="June" />
											</record>
											<record id="7">
												<translation id="name" translation_id="249624" type="use" value="July" />
											</record>
											<record id="8">
												<translation id="name" translation_id="249625" type="use" value="August" />
											</record>
											<record id="9">
												<translation id="name" translation_id="249626" type="use" value="September" />
											</record>
											<record id="10">
												<translation id="name" translation_id="249627" type="use" value="October" />
											</record>
											<record id="11">
												<translation id="name" translation_id="249628" type="use" value="November" />
											</record>
											<record id="12">
												<translation id="name" translation_id="249629" type="use" value="December" />
											</record>
										</list>
									</widget>

									<widget class="popup_button" auto_size="all" id="yeap" sele="sdby">
										<translation id="text" translation_id="469468" type="use" value="Select Year[COMMENT: EULA dialog; bug 185228]" />
									</widget>

								</container>

							</container>
							
						</container>

				

					</container>

					<container>
						<attachment_group class="button_arrangement_right" />

						<widget class="primary_icon_tick_button" id="ok">
							<translation id="text" translation_id="243222" type="use" value="Continue" />
						</widget>

					</container>
				</container>

			</container>
		</container>

		<container id="eptP">
			<layout class="arrange_vertical_attachment" alignment="bottom,extend" offset="0" />

			<widget class="web_panel" id="webP">
				<layout class="stick_to_sides_attachment" alignment="horizontal" />
			</widget>

			<widget class="button" auto_size="all">
				<layout class="stick_to_sides_attachment" alignment="right" />
				<event id="click_event" event_id="swSc" selection="selc" />
				<translation id="text" type="use" translation_id="247672" value="Back" />
			</widget>
		</container>

	</container>


</panel>