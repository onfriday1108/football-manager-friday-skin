					</list>

					<list id="column_table_properties">
						<record indx="0">
							<integer id="left" value="8" />
						</record>
						<record indx="1">
							<flags id="alignment" value="left,centre_y" />
							<integer id="top" value="2" />
							<integer id="bottom" value="2" />
						</record>
					</list>
				</record>

				<record id="object_property">
