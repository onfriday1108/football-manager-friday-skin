				<integer value="c3ln" />
				<integer value="crep" />
				<integer value="cmnl" />
				<integer value="cmxl" />
				<integer value="cmnw" />
				<integer value="cmxw" />
				<integer value="cuae" />
				<integer value="csso" />
				<integer value="wbcl" />
				<integer value="wbst" />
				<integer value="wben" />
			</list>

		</widget>

	</container>


</panel>