<!--This file is used for person property PsnC as the xml for a PROPERTY_LIST_VIEWER_XML_PANEL-->
<panel>
  <layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true" inset="0"/>

  
  <widget class="picture" id="bgpi" scale_picture="false" image_alignment="centre">
    <record id="object_property">
      <list id="get_properties">
        <record>
          <integer id="get_property" value="file" />
          <integer id="set_property" value="file" />
        </record>
        <record>
          <integer id="get_property" value="rrep" />
          <integer id="set_property" value="rrep" />
        </record>
      </list>
    </record>
  </widget>

  <widget class="label" id="labw" alignment="centre">
    <record id="object_property">
      <list id="get_properties">
        <record>
          <integer id="get_property" value="text" />
          <integer id="set_property" value="text" />
        </record>
        <record>
          <integer id="get_property" value="colr" />
          <integer id="set_property" value="colr" />
        </record>
      </list>
    </record>
  </widget>
</panel>
