<panel height="50">
	<layout class="stick_to_sides_attachment" alignment="all" inset="2,0" apply_to_children="true" />

	<container class="main_contrast_box" id="sBox"  wants_mouse_events="false">
		<layout class="arrange_horizontal_attachment" alignment="left,extend" offset="4" gap="4"  />
		<layout class="stick_to_sides_attachment" alignment="vertical" inset="0" apply_to_children="true" />
		<container width="25">
			<layout class="stick_to_sides_attachment" alignment="all" inset="0,2" apply_to_children="true" />
			<widget class="picture" id="pict" red_replacement="white" image_alignment="centre" keep_aspect_ratio="true" wants_mouse_events="false"/>
			<widget class="object_portrait_picture" id="matP" red_replacement="white" image_alignment="centre" keep_aspect_ratio="true"  scale_picture="true" hidden="true" />
		</container>

		<widget class="text" id="text" style="semi_bold" alignment="centre_y, left" ahOM="false"  wants_mouse_events="false" />
	</container>
	<widget class="button" id="SesB" show_button_appearance="false" />
</panel>