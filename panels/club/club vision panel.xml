<panel>
	<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true"/>

	<container>
		<attachment_group class="vertical_arrange" vertical_alignment="bottom, extend" vertical_offset="0" vertical_gap="8"/>

		<container class="scrolling_box">
			<container draw_horizontal_dividers="true">
				<attachment_group class="vertical_arrange_autosize" vertical_gap="0">
					<global id="vertical_gap" name="ui.inner_box_gap"/>
				</attachment_group>

				<!-- club culture -->
				<container class="inner_box">
					<attachment_group class="vertical_arrange_autosize"/>

					<container class="club_vision_objective_list_panel" id="ccbx" title_colour="title"/>
				</container>

				<container class="inner_box">
					<attachment_group class="vertical_arrange_autosize" vertical_gap="8"/>

					<widget class="text" font="title" colour="accent" auto_size="vertical">
						<translation id="text" translation_id="491859" type="use" value="5 Year Plan[COMMENT: AREA: Club vision panel; SUB-AREA: 5 year plan sub-panel heading; FR-BN: 23335; TONE: Formal; CHARLIMIT: None; REF-ID: n/a; NOTE: Title string for the 5 year plan section of the club vision panel]"/>
					</widget>

					<!-- ongoing -->
					<container class="inner_box">
						<attachment_group class="vertical_arrange_autosize"/>

						<container class="club_vision_objective_list_panel" id="onbx" title_colour="text"/>
					</container>

					<widget class="horizontal_subtle_divider"/>

					<container class="inner_box">
						<attachment_group class="vertical_arrange_autosize" vertical_gap="0"/>

						<!-- current season -->
						<container class="inner_box" appearance="boxes/custom/vision/timeline/top/paper" >
							<attachment_group class="vertical_arrange_autosize" vertical_offset="0" vertical_gap="8" horizontal_alignment="horizontal" horizontal_inset="0"/>

							<container class="club_vision_objective_list_panel" id="csbx" title_colour="text"/>
							<widget class="horizontal_subtle_divider"/>
						</container>

						<!-- current season + 1 -->
						<container class="inner_box" appearance="boxes/custom/vision/timeline/middle/paper" >
							<attachment_group class="vertical_arrange_autosize" vertical_offset="0" vertical_gap="8" horizontal_alignment="horizontal" horizontal_inset="0"/>

							<container class="club_vision_objective_list_panel" id="s1bx" title_colour="text"/>
							<widget class="horizontal_subtle_divider"/>
						</container>

						<!-- current season + 2 -->
						<container class="inner_box" appearance="boxes/custom/vision/timeline/middle/paper" >
							<attachment_group class="vertical_arrange_autosize" vertical_offset="0" vertical_gap="8" horizontal_alignment="horizontal" horizontal_inset="0"/>

							<container class="club_vision_objective_list_panel" id="s2bx" title_colour="text"/>
							<widget class="horizontal_subtle_divider"/>
						</container>

						<!-- current season + 3 -->
						<container class="inner_box" appearance="boxes/custom/vision/timeline/middle/paper" >
							<attachment_group class="vertical_arrange_autosize" vertical_offset="0" vertical_gap="8" horizontal_alignment="horizontal" horizontal_inset="0"/>

							<container class="club_vision_objective_list_panel" id="s3bx" title_colour="text"/>
							<widget class="horizontal_subtle_divider"/>
						</container>

						<!-- current season + 4 -->
						<container class="inner_box" appearance="boxes/custom/vision/timeline/bottom/paper" >
							<attachment_group class="vertical_arrange_autosize" vertical_offset="0" vertical_gap="8" horizontal_alignment="horizontal" horizontal_inset="0"/>

							<container class="club_vision_objective_list_panel" id="s4bx" title_colour="text"/>
							<widget class="horizontal_subtle_divider"/>
						</container>
					</container>
				</container>
			</container>
		</container>

		<container id="btns" height="35" hidden="true">
			<attachment_group class="horizontal_arrange_vertical_autosize" horizontal_alignment="left, fill" horizontal_offset="0" vertical_inset="0"/>

			<container>
				<attachment_group class="horizontal_arrange_autosize_all" horizontal_alignment="left">
					<global id="horizontal_gap" name="ui.button_row_gap"/>
				</attachment_group>

				<widget class="popup_button" id="adob" select_event="adob" fixed="true" auto_size="all" icon="icons/16px/plus">
					<translation id="text" translation_id="491860" type="use" value="Add Competition Objective[COMMENT: AREA: Club vision meeting panel; SUB-AREA: action button area; FR-BN: 16574; TONE: Formal; CHARLIMIT: None; REF-ID: n/a; NOTE: Menu button label for user to choose a new competition objective to add to the club vision during a meeting]"/>
				</widget>

				<widget class="subtle_button" id="revt" click_event="revt" auto_size="all">
					<translation id="text" translation_id="344717" type="use" value="Revert"/>
				</widget>

				<widget class="subtle_button" id="undo" click_event="UNDO" auto_size="all">
					<translation id="text" translation_id="230251" type="use" value="Undo"/>
				</widget>
			</container>

			<container>
				<attachment_group class="button_arrangement_right"/>

				<widget class="primary_button" id="sgtm" click_event="SuVC">
					<translation id="text" translation_id="347325" type="use" value="Suggest"/>
				</widget>

				<widget class="primary_icon_tick_button" id="set " click_event="set ">
					<translation id="text" translation_id="229918" type="use" value="Confirm"/>
				</widget>
			</container>
		</container>
	</container>

</panel>